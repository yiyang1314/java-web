<HTML xmlns="http://www.w3.org/1999/xhtml">
<HEAD>
	<base href="<%=basepath%>" />
	<TITLE>自如租房 - 用户管理</TITLE>
	<META content="text/html; charset=utf-8" http-equiv=Content-Type><LINK 
	rel=stylesheet type=text/css href="file/css/style.css">
	
	<META name=GENERATOR >
	<script type="text/javascript">
			function update(id){
				if(confirm("您确定要修改"+id+"吗？")){
					location.href="house_updateById?houseid="+id;
				}
				   
			}
			function del(id){
				if(confirm("您确定要删除"+id+"吗？")){
					location.href="house_delete?houseid="+id;
				}
				   
			}
		</script>

</HEAD>
<BODY>
<DIV id=header class=wrap>
<DIV id=logo>
<IMG src="file/images/logo.jpg"></DIV>
<DIV class=search>
<LABEL class="ui-green searchs">
<a href="pages/house/fabu.jsp" title="">发布房屋信息</a>
</LABEL> 
<LABEL class="ui-green searchs">
<a href="house_page" title="">查看房源列表</a>
</LABEL> 
<LABEL class=ui-green>
<INPUT onclick='document.location="users_logout"' value="退       出" type=button name=search></LABEL> 
</DIV>
</DIV>
<DIV class="main wrap">
		<DIV id=houseArea>
			<TABLE class=house-list>
				<TBODY>
					<c:forEach items="${houselist }" var="house" varStatus="pionter">
					<TR ${pionter.count%2==0?'class=add':'' }>
						<TD class=house-thumb>
							<SPAN>
								<A href="house_findById?houseid=${house.houseid }" target="_blank">
									<img src="file/images/thumb_house.gif" width="100" height="75" alt="">
								</A>
							</SPAN>
						</TD>
						<TD>
							<DL>
								<DT>
									<A href="house_findById?houseid=${house.houseid }" target="_blank"> ${house.title }</A>
								</DT>
								<DD>
									${house.street} ${house.region['regionNameC']} ${house.houseArea} 平米<BR>联系方式：${house.phone}
								</DD>
							</DL>
						</TD>
						<TD class=house-type>
						<LABEL class=ui-green> 
						<INPUT onclick="update('${house.houseid}')" value="修    改" type=button name=search1>
						</LABEL>
						</TD>
						<TD class=house-price>
						<LABEL class=ui-green> 
						<INPUT value="删    除" type=button name=search2 onclick="del('${house.houseid}')">
						</LABEL>
						</TD>
					</TR>
			</c:forEach>
				</TBODY>
			</TABLE>
		</DIV>
		<DIV class=pager>

		<jsp:include page="../page/page.jsp">
			<jsp:param value="house_admin" name="action"/>
			<jsp:param value="${count}" name="count"/>
		</jsp:include>
	</DIV>
</DIV>
<DIV id=footer class=wrap>
<DL>
